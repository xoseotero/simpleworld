# Minimum signed value.
# IN:  g0 first value
# IN:  g1 second value
# OUT: g0 minimum value
.label std_min
        blt g1 g0 _std_min_second

        .label _std_min_first
        ret

        .label _std_min_second
        move g0 g1
        ret
