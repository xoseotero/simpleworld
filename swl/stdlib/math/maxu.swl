# Maximum signed value of a block of memory.
# IN:  g0 first value
# IN:  g1 second value
# OUT: g0 maximum value
.label std_maxu
        bgtu g1 g0 _std_maxu_second

        .label _std_maxu_first
        ret

        .label _std_maxu_second
        move g0 g1
        ret
