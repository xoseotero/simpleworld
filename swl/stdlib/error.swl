# Definitions for the errors
.define STD_NOERROR           0x0000    # No error

.define STD_EINVAL            0x0001    # Invalid argument
.define STD_EFAULT            0x0002    # Bad address

.define STD_EOVERFLOW         0x0003    # Value too large

.define STD_ENOMEM            0x0004    # Not enough space
