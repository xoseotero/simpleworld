# Push all the general purpose registers (all except pc, sp and cs)
.label stack_push
	push r0
	push r1
	push r2
	push r3
	push r4
	push r5
	push r6
	push r7
	push r8
	push r9
	push r10
	push r11

	ret



# Pop all the general purpose register (all except pc, sp and cs)
.label stack_pop
	push r11
	push r10
	push r9
	push r8
	push r7
	push r6
	push r5
	push r4
	push r3
	push r2
	push r1
	push r0

	ret
