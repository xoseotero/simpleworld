# Test code for source_test.cpp

.include "defines.swl"

# Loop
.label init
       # load the initial values
       loadi r1 VALUE1
       loadi r2 VALUE2

       # some operations
       add r0 r1 r2
       move r0 r1
       swap r2 r2

       push r0
       div r0 r1 r2
       pop r0
       rri r0 r0 0x000F

       # restart the CPU
       RESTART

       b init		# loop

# 4 bytes for the stack
.label stack
.block 0x0004
